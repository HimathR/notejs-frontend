  <React.Fragment key={cell.id}>\r\n      <AddCell nextCellId={cell.id} />\r\n      <CellListItem cell={cell} />\r\n    </React.Fragment>\r\n  ));\r\n\r\n  return (\r\n    <div className=\"cell-list\">\r\n      {renderedCells}\r\n      <AddCell nextCellId={null} />\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default CellList;\r\n","/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/state/index.ts",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/hooks/useTypedSelector.ts",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/components/cell-list-item.tsx",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/components/add-cell.tsx",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/state/store.ts",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/state/cell.ts",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/hooks/use-actions.ts",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/components/code-cell.tsx",["220"],"import { useEffect } from \"react\";\nimport CodeEditor from \"./code-editor\";\nimport Preview from \"./preview\";\nimport Resizable from \"./resizable\";\nimport { Cell } from \"../state\";\nimport { useActions } from \"../hooks/use-actions\";\nimport { useTypedSelector } from \"../hooks/useTypedSelector\";\nimport { LoadingButton } from \"@atlaskit/button\";\nimport \"./code-cell.css\";\nimport { useCumulativeCode } from \"../hooks/use-cumulative-code\";\n\ninterface CodeCellProps {\n  cell: Cell;\n}\n\nconst CodeCell: React.FC<CodeCellProps> = ({ cell }) => {\n  const { updateCell, createBundle } = useActions();\n  // go through state object and find the cell with the id of the cell we are currently working with\n  const bundle = useTypedSelector((state) => state.bundles[cell.id]);\n  const cumulativeCode = useCumulativeCode(cell.id);\n\n  useEffect(() => {\n    // debouncing the bundle function\n    if (!bundle) {\n      // this ensures that bundle is created as soon as the cell is created\n      createBundle(cell.id, cumulativeCode);\n      return;\n    }\n    const timer = setTimeout(async () => {\n      createBundle(cell.id, cumulativeCode);\n    }, 750);\n\n    return () => {\n      clearTimeout(timer);\n    };\n  }, [cumulativeCode, cell.id, createBundle]);\n\n  return (\n    <Resizable direction=\"vertical\">\n      <div style={{ height: \"100%\", display: \"flex\", flexDirection: \"row\" }}>\n        <Resizable direction=\"horizontal\">\n          <CodeEditor\n            initialValue={cell.content}\n            onChange={(value) => updateCell(cell.id, value)}\n          />\n        </Resizable>\n        <div className=\"progress-wrapper\">\n          {!bundle || bundle.loading ? (\n            <div className=\"progress-cover\">\n              <LoadingButton appearance=\"primary\" isLoading>\n                Loading\n              </LoadingButton>\n            </div>\n          ) : (\n            //use bundle && to make sure code and error is not shown if undefined\n            <Preview code={bundle.code} err={bundle.err} />\n          )}\n        </div>\n      </div>\n    </Resizable>\n  );\n};\n\nexport default CodeCell;\n","/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/components/text-editor.tsx",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/components/action-bar.tsx",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/state/reducers/index.ts",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/state/action-creators/index.ts",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/hooks/use-cumulative-code.ts",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/components/preview.tsx",[],"/mnt/c/Users/User/Desk