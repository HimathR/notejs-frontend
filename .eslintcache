=\"horizontal\">\n          <CodeEditor\n            initialValue={cell.content}\n            onChange={(value) => updateCell(cell.id, value)}\n          />\n        </Resizable>\n        <div className=\"progress-wrapper\">\n          {!bundle || bundle.loading ? (\n            <div className=\"progress-cover\">\n              <LoadingButton appearance=\"primary\" isLoading>\n                Loading\n              </LoadingButton>\n            </div>\n          ) : (\n            //use bundle && to make sure code and error is not shown if undefined\n            <Preview code={bundle.code} err={bundle.err} />\n          )}\n        </div>\n      </div>\n    </Resizable>\n  );\n};\n\nexport default CodeCell;\n","/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/components/text-editor.tsx",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/components/action-bar.tsx",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/state/reducers/index.ts",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/state/action-creators/index.ts",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/hooks/use-cumulative-code.ts",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/components/preview.tsx",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/components/resizable.tsx",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/components/code-editor.tsx",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/state/reducers/bundlesReducer.ts",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/state/reducers/cellsReducer.ts",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/state/action-types/index.ts",[],"/mnt/c/Users/User/Desktop/ReactTS Projects/notejs/packages/local-client/src/bundler/